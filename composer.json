{
    "name": "itmaster/cms-base",
    "description": "It Master CMS base project",
    "keywords": ["yii2", "framework", "cms"],
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "yiisoft/yii2": ">=2.0.13",
        "yiisoft/yii2-bootstrap": "^2.0",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-jui": "^2.0",
        "2amigos/yii2-ckeditor-widget": "^2.1",
        "yiisoft/yii2-imagine": "^2.0",
        "yiisoft/yii2-authclient": "*",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-sortable": "*",
        "kartik-v/yii2-sortable-input": "*",
        "2amigos/yii2-gallery-widget": "~1.0",
        "kartik-v/yii2-mpdf": "*",
        "kartik-v/yii2-editable": "^1.7",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "kartik-v/yii2-widget-datepicker": "@dev",
        "kartik-v/yii2-datecontrol": "=1.9.5",
        "wbraganca/yii2-dynamicform": "dev-master",
        "kartik-v/yii2-widget-timepicker": "*",
        "kartik-v/yii2-widget-depdrop": "@dev",
        "philippfrenzel/yii2fullcalendar":"*",
        "kartik-v/yii2-money": "*",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-widget-colorinput": "*",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "kartik-v/yii2-tree-manager": "@dev",
        "kartik-v/yii2-export": "*",
        "hbattat/verifyemail": "1.0.7",
        "kartik-v/yii2-tabs-x": "*",
        "nesbot/carbon": "~1.18"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
